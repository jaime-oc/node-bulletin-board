    agent any

    stages {
        stage("build") {
            steps {
                echo 'build stage is running branch 2'
            }
        }

        stage("test") {
            steps {
                echo 'test stage is running branch 2'
            }
        }

        stage("deploy") {
            steps {
                echo 'deploy stage is running branch 2'
            }
        }
    }
}
